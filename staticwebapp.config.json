{
  "routes": [
     {
       "route": "/login",
       "redirect": "/.auth/login/aad"
     },
     {
       "route": "/logout",
       "redirect": "/.auth/logout"
     },
     {
       "route": "/.auth/login/github",
       "statusCode": 404
     },
     {
       "route": "/admin",
       "allowedRoles": ["administrator"]
     },
     {
       "route": "/contributor",
       "allowedRoles": ["contributor"]
     },
     {
      "route": "/reader",
      "allowedRoles": ["reader"]
    },
    {
      "route": "/profile",
      "allowedRoles": ["reader, contributor, administrator, anonymous, authenticated"]
    }
   ],
   "networking": {
    "allowedIpRanges": [
      "10.0.0.0/24",
      "100.0.0.0/32",
      "192.168.100.0/22",
      "176.177.25.47/32"
    ]
  }
}
